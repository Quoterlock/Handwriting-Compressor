<Window x:Class="HandwritingCompressor.EnterProductKey"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HandwritingCompressor"
        mc:Ignorable="d"
        Title="EnterProductKey" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="productKeyTextBox"/>
            <Button x:Name="activateBtn" Click="activateBtn_Click">Activate</Button>
        </StackPanel>
    </Grid>
</Window>
